[case testVarArgsNumber]
from typing import Unpack
def takes_exactly_3(a1: str, a2: str, a3: str, /) -> None: ...
def takes_at_least3(a1: str, a2: str, a3: str, /, *args: str) -> None: ...

def test_takes_at_least3(
    x0: tuple[Unpack[tuple[str, ...]]],
    x1: tuple[str, Unpack[tuple[str, ...]]],
    x2: tuple[str, str, Unpack[tuple[str, ...]]],
    x3: tuple[str, str, str, Unpack[tuple[str, ...]]],
    x4: tuple[str, str, str, str, Unpack[tuple[str, ...]]],
) -> None:
    takes_at_least3(*x0)
    takes_at_least3(*x1)
    takes_at_least3(*x2)
    takes_at_least3(*x3)
    takes_at_least3(*x4)

def test_takes_exactly_3(
    x0: tuple[Unpack[tuple[str, ...]]],
    x1: tuple[str, Unpack[tuple[str, ...]]],
    x2: tuple[str, str, Unpack[tuple[str, ...]]],
    x3: tuple[str, str, str, Unpack[tuple[str, ...]]],
    x4: tuple[str, str, str, str, Unpack[tuple[str, ...]]],
) -> None:
    takes_exactly_3(*x0)
    takes_exactly_3(*x1)
    takes_exactly_3(*x2)
    takes_exactly_3(*x3)
    takes_exactly_3(*x4)  # E: Too many positional arguments for "takes_exactly_3"
