[case testStarArgsWithPrefixAndSuffix]

class P: ...
class V: ...
class S: ...

def f(
    a1: P, a2: P, /,                      # prefix
    *args: *tuple[*tuple[V, ...], S, S],  # unbounded + suffix
) -> None: pass

def test_good_cases1(
    p: P,
    v: tuple[V, ...],
    s: S,
) -> None:
    # check all possible bracketing combinations
    # there are 45 possible ways to parenthesize the arguments
    # (little Schroeder number, see https://oeis.org/A001003)
    f(p, p, *v, s, s)
    f(p, p, *v, *(s, s))
    f(p, p, *(*v, s), s)
    f(p, p, *(*v, s, s))
    f(p, p, *(*v, *(s, s)))
    f(p, p, *(*(*v, s), s))
    f(p, *(p, *v), s, s)
    f(p, *(p, *v), *(s, s))
    f(p, *(p, *v, s), s)
    f(p, *(p, *(*v, s)), s)
    f(p, *(*(p, *v), s), s)
    f(p, *(p, *v, s, s))
    f(p, *(p, *v, *(s, s)))
    f(p, *(p, *(*v, s), s))
